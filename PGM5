// program to find minimum element in rotated sorted array 

#include<iostream>
#include<climits>
#include<algorithm>
using namespace std;
int main(){
    int arr[]={11,13,15,17};
    int n=sizeof(arr)/sizeof(arr[0]);
    int minimum=INT_MAX;
    for(int i=0;i<n;i++){
        if(arr[i]<minimum){
            minimum=arr[i];
        }
    }
    cout<<"Minimum element is "<<minimum;
    return 0;
}
